DEBUG=1
# ALLOW_DEBUG_INACTIVE_LOGIN=True
# Frontend dev proxy (Vite):
# VITE_API_PROXY_TARGET=http://127.0.0.1
# VITE_WS_PROXY_TARGET=http://127.0.0.1
DJANGO_SECRET_KEY=change-me-in-production-use-long-random-string
# ALLOWED_HOSTS=localhost,127.0.0.1,[::1],yourdomain.com
# DATABASE_URL=postgres://user:password@db:5432/maestro_db
# DB_CONN_MAX_AGE=60

# CORS (production: set to False and list origins)
# CORS_ALLOW_ALL_ORIGINS=False
# CORS_ALLOWED_ORIGINS=https://yourdomain.com,https://www.yourdomain.com
# CSRF_TRUSTED_ORIGINS=https://yourdomain.com,https://www.yourdomain.com

# Redis / cache (required in production)
REDIS_URL=redis://redis:6379/0
# CACHE_URL=redis://redis:6379/2
# USE_REDIS_CACHE=True
# CACHE_KEY_PREFIX=maestro

# Email (production SMTP)
# EMAIL_HOST=smtp.gmail.com
# EMAIL_PORT=587
# EMAIL_USE_TLS=True
# EMAIL_HOST_USER=your-email@gmail.com
# EMAIL_HOST_PASSWORD=your-google-app-password
# DEFAULT_FROM_EMAIL=noreply@maestro.uz

# Frontend URL (for password reset links in emails)
FRONTEND_URL=http://localhost:5173

# S3 Compatible Object Storage (AWS, Yandex, Cloudflare R2)
# Required in production when DEBUG=0 to store media files
AWS_ACCESS_KEY_ID=your_access_key
AWS_SECRET_ACCESS_KEY=your_secret_key
AWS_STORAGE_BUCKET_NAME=your_bucket_name
AWS_S3_REGION_NAME=us-east-1
# AWS_S3_ENDPOINT_URL=https://s3.yandexcloud.net (Optional, use for Yandex/Cloudflare etc.)
# AWS_S3_CUSTOM_DOMAIN=cdn.maestro.uz (Optional, use if you have CDN)

# Gemini AI
GEMINI_API_KEY=your-gemini-api-key-here

# Payment Gateways (Payme / Click)
# PAYME_MERCHANT_ID=your_payme_merchant_id
# PAYME_SECRET_KEY=your_payme_secret_key
# CLICK_MERCHANT_ID=your_click_merchant_id
# CLICK_SERVICE_ID=your_click_service_id
# CLICK_SECRET_KEY=your_click_secret_key

# API Rate Limits (override if needed)
# THROTTLE_AUTH_REGISTER=5/hour
# THROTTLE_AUTH_VERIFY_EMAIL=20/hour
# THROTTLE_AUTH_RESEND_VERIFICATION=5/hour
# THROTTLE_AUTH_LOGIN=10/minute
# THROTTLE_AUTH_REFRESH=30/minute
# THROTTLE_AUTH_FORGOT_PASSWORD=5/hour
# THROTTLE_AUTH_RESET_PASSWORD=10/hour
# THROTTLE_CHAT_HTTP_SEND=120/minute

# WebSocket anti-flood
# CHAT_WS_RATE_LIMIT=30
# CHAT_WS_RATE_WINDOW_SECONDS=10
# CHAT_WS_MAX_TEXT_LENGTH=2000
